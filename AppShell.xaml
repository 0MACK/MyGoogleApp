<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MyGoogleApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MyGoogleApp"
    Shell.FlyoutBehavior="Disabled"
    Title="MyGoogleApp">

    
    
    <TabBar>
        <Tab Title="Discover" Icon="discover.png">
            <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        </Tab>

        <Tab Title="Search" Icon="search.png">
            <ShellContent
                Title="Search"
                ContentTemplate="{DataTemplate local:App}"
                Route="Search" />
        </Tab>
        <Tab Title="Saved" Icon="saved.png">
            <ShellContent
            Title="Saved"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="Saved" />
        </Tab>

    </TabBar>

</Shell>
